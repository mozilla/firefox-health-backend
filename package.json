{
  "name": "firefox-health-backend",
  "version": "0.0.1",
  "description": "Firefox Health backend",
  "main": "build/index.js",
  "scripts": {
    "test:src": "neutrino test",
    "test:lint": "neutrino lint",
    "coverage": "nyc neutrino test && nyc report --reporter=text-lcov | coveralls",
    "test": "npm-run-all --parallel test:*",
    "watch": "neutrino test --watch",
    "build": "neutrino build",
    "start": "neutrino start",
    "start:prod": "neutrino build && node .",
    "start:debugger": "neutrino build && node --inspect .",
    "precommit": "lint-staged",
    "heroku-postbuild": "npm run build"
  },
  "lint-staged": {
    "*.js": [
      "neutrino lint"
    ]
  },
  "author": "Harald Kirschner <npm@digitarald.com> (http://digitarald.de/)",
  "license": "MPL-2.0",
  "dependencies": {
    "async-redis": "^1.1.4",
    "browserslist": "^2.2.2",
    "caniuse-db": "^1.0.30000712",
    "cheerio": "^1.0.0-rc.2",
    "chrono-node": "^1.3.5",
    "dotenv": "^4.0.0",
    "googleapis": "^27.0.0",
    "ical-with-secure-request": "^0.6.0",
    "isomorphic-fetch": "^2.2.1",
    "json2csv": "^3.11.0",
    "koa": "^2.3.0",
    "koa-cors": "0.0.16",
    "koa-logger": "^2.0.1",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.2.1",
    "lodash": "^4.17.2",
    "moment": "^2.19.3",
    "qs": "^6.5.0",
    "query-string": "^5.0.1",
    "simple-statistics": "^5.2.0"
  },
  "devDependencies": {
    "@neutrinojs/airbnb-base": "^8.3.0",
    "@neutrinojs/mocha": "^8.3.0",
    "@neutrinojs/node": "^8.3.0",
    "coveralls": "^3.0.0",
    "fetch-mock": "^6.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "mocha-lcov-reporter": "^1.3.0",
    "neutrino": "^8.3.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^11.1.0",
    "source-map-support": "^0.5.3",
    "supertest": "^3.0.0"
  }
